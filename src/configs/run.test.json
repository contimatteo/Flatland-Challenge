[
  {
    "debug": true,
    "n_agents": 2,
    "config_uuid": "testing-prova-1",

    "rail-env": {
      "map_width": 28,
      "map_height": 21,
      "n_cities": 2,
      "max_rails_in_city": 1,
      "max_rails_between_cities": 1,
      "cities_grid_distribution": false,
      "malfunction_rate": 0.0001,
      "malfunction_min_duration": 15,
      "malfunction_max_duration": 50
    },

    "emulator": {
      "active": true,
      "window_width": 1200,
      "window_height": 1200,
      "step_timebreak_seconds": 0
    },

    "policy": {
      "type": "eps-greedy",
      "parameters": { "eps": 0.1 }
    },

    "agent": {
      "type": "dqn",
      "memory_limit": 10000,
      "parameters": {
        "dueling_type": "avg",
        "nb_steps_warmup": 100,
        "target_model_update": 200,
        "enable_double_dqn": false,
        "enable_dueling_network": false
      }
    },

    "network": {
      "type": "sequential-2",
      "parameters": {},
      "metrics": ["mae", "accuracy"],
      "optimizer": {
        "type": "adam",
        "parameters": {}
      }
    }
  },
  {
    "train": {
      "verbose": 2,
      "n_min_attempts": 5,
      "log_interval": 500,
      "nb_max_episode_steps": 1000,
      "callbacks": []
    }
  },
  {
    "test": {
      "verbose": 2,
      "n_attempts": 1,
      "nb_max_episode_steps": 1000,
      "callbacks": [
        {
          "type": "wandb",
          "parameters": {}
        }
      ]
    }
  }
]
